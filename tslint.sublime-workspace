(resources)\\n\\t\\tthis.planetResources = resources;\\n}\\n\\nfunction createMenus () {\\n\\n\\tshowShipsSelector.call(this)\\n\\t.then(showResourcesSelector.bind(this))\\n\\t.then(endOrder.bind(this));\\n}\\n\\nfunction showShipsSelector () {\\n\\tvar _this = this;\\n\\n\\treturn new Promise(function (resolve, reject) {\\n\\t\\t_this.dom.removeAll();\\n\\n\\t\\tvar template = new GlobalMenuFleetSubdivideShipsTemplate(_this.fleetData, resolve, function () {\\n\\t\\t\\t_this.onFinish(false);\\n\\t\\t\\treject();\\n\\t\\t});\\n\\n\\t\\t_this.dom.add(template.getDom());\\n\\t});\\n}\\n\\nfunction showResourcesSelector (ships) {\\n\\tvar _this = this;\\n\\n\\treturn new Promise(function (resolve, reject) {\\n\\t\\t_this.dom.removeAll();\\n\\n\\t\\tvar minResources = getMinResources.call(_this, _this.fleetData, ships);\\n\\t\\tvar maxResources = getMaxResources.call(_this, _this.fleetData, ships, _this.planetResources);\\n\\t\\tvar capacity = getShipsCapacity(ships);\\n\\n\\t\\tvar template = new GlobalMenuFleetSubdivideResourcesTemplate(capacity, minResources, maxResources, function (resources) {\\n\\t\\t\\tresolve({ships: ships, resources: resources});\\n\\t\\t}, function () {\\n\\t\\t\\treject();\\n\\t\\t\\tcreateMenus.call(_this);\\n\\t\\t});\\n\\n\\t\\t_this.dom.add(template.getDom());\\n\\t});\\n}\\n\\nfunction endOrder (data) {\\n\\tvar _this = this;\\n\\tvar resources = data.resources;\\n\\tvar ships = data.ships;\\n\\tthis.dom.removeAll();\\n\\n\\tvar template = new GlobalMenuFleetSubdivideResourcesFinishOrder(this.fleetData, ships, resources, function () {\\n\\t\\t_this.onFinish(true, ships, resources);\\n\\t}, function () {\\n\\t\\tshowResourcesSelector.call(_this, ships)\\n\\t\\t.then(endOrder.call(_this));\\n\\t});\\n\\n\\t_this.dom.add(template.getDom());\\n}\\n\\nfunction getResourcesQuantity (resources) {\\n\\tvar total = 0;\\n\\n\\tif(!resources)\\n\\t\\treturn 0;\\n\\n\\tfor (var i = resources.length - 1; i >= 0; i--) {\\n\\t\\ttotal += resources[i];\\n\\t}\\n\\n\\treturn total;\\n}\\n\\nfunction getMinResources (originalFleet, newShips) {\\n\\tvar originalResourcesQuantity = getResourcesQuantity(originalFleet.resources);\\n\\tvar originalCapacity = getShipsCapacity(originalFleet.ships);\\n\\tvar newCapacity = getShipsCapacity(newShips);\\n\\n\\tvar remainingCapacity = originalCapacity - newCapacity;\\n\\n\\tif(originalResourcesQuantity <= remainingCapacity ) return packResources(0, 0, 0, 0, 0, 0, 0);\\n\\n\\tvar orphanResources = originalResourcesQuantity - remainingCapacity;\\n\\n\\treturn distributeQuantityBetweenResources(orphanResources, originalFleet.resources);\\n}\\n\\nfunction distributeQuantityBetweenResources (quantity, resources) {\\n\\tvar perResource = Math.floor(quantity / 7);\\n\\tvar orphanResources = quantity - perResource * 7;\\n\\n\\tvar distributed = packResources(resources.n, resources.a, resources.b, resources.c, resources.d, resources.e, resources.f);\\n\\n\\tdistributed.n -= orphanResources;\\n\\tfor(var type in distributed)\\n\\t\\tdistributed[type] -= perResource;\\n\\n\\treturn resditributeNegativeResources(distributed);\\n}\\n\\nfunction resditributeNegativeResources (resources) {\\n\\tfor(var type in resources){\\n\\t\\tif(resources[type] >= 0) continue;\\n\\n\\t\\tvar negativeQuantity = !resources[type];\\n\\t\\tvar result = restResources(resources, negativeQuantity);\\n\\t\\tif(!result) return false;\\n\\t}\\n}\\n\\nfunction restResources (resources, quantity) {\\n\\tfor(var type in resources){\\n\\t\\tvar resource = resources[type];\\n\\t\\tif(resource <= 0) continue;\\n\\n\\t\\tif(resource < quantity){\\n\\t\\t\\tresources[type] = 0;\\n\\t\\t\\tquantity -= resource;\\n\\t\\t\\tcontinue;\\n\\t\\t}\\n\\n\\t\\tresources[type] -= quantity;\\n\\t\\tquantity = 0;\\n\\t}\\n}\\n\\nfunction getShipsCapacity (ships) {\\n\\tvar capacity = 0;\\n\\n\\tfor(var type in ships) {\\n\\t\\tvar quantity = ships[type];\\n\\t\\tcapacity += quantity * getShipTypeCargo(type);\\n\\t}\\n\\n\\treturn capacity;\\n}\\n\\nfunction getMaxResources (originalFleet, newShips, planetResources) {\\n\\tvar resources = packResources(0, 0, 0, 0, 0, 0, 0);\\n\\n\\tfor (var type in resources){\\n\\n\\t\\tvar planetQuantity = planetResources ? planetResources[type].quantity : 0;\\n\\t\\tvar fleetResources = originalFleet.resources[type];\\n\\n\\t\\tresources[type] = planetQuantity + fleetResources;\\n\\t}\\n\\n\\treturn resources;\\n}\\n\\nfunction packResources (n, a, b, c, d, e, f) {\\n\\treturn {\\n\\t\\tn: n,\\n\\t\\ta: a,\\n\\t\\tb: b,\\n\\t\\tc: c,\\n\\t\\td: d,\\n\\t\\te: e,\\n\\t\\tf: f\\n\\t};\\n}\\n\\nfunction getShipTypeCargo (type) {\\n\\treturn 1;\\n}\\n\",\"'use strict';\\n\\nvar d = require('dom-wrapper');\\n\\nmodule.exports = FinihsOrder;\\n\\nfunction FinihsOrder (originalFleet, ships, resources, okCallback, cancelCallback) {\\n\\tthis.ships = ships;\\n\\tthis.originalFleet = originalFleet;\\n\\tthis.resources = resources;\\n\\tthis.okCallback = okCallback;\\n\\tthis.cancelCallback = cancelCallback;\\n\\tthis.references = {};\\n}\\n\\nFinihsOrder.prototype.getDom = function() {\\n\\tvar dom = d.div('subdivide-finish').add(\\n\\t\\td.div('content').add(\\n\\t\\t\\td.div('ships').add(\\n\\t\\t\\t\\td.p('title').text('Unidades'),\\n\\t\\t\\t\\tthis.getShipsCards()\\n\\t\\t\\t),\\n\\t\\t\\td.div('resources').add(\\n\\t\\t\\t\\td.p('title').text('Recursos'),\\n\\t\\t\\t\\tthis.getResourcesCards(),\\n\\t\\t\\t\\td.div('total-cargo').add(\\n\\t\\t\\t\\t\\td.p('quantity').text(this.getTotalCargo()),\\n\\t\\t\\t\\t\\td.p('max').text(' /' + this.getTotalSpace())\\n\\t\\t\\t\\t)\\n\\t\\t\\t),\\n\\t\\t\\td.div('summary').add(\\n\\t\\t\\t\\td.div('title').text('Resumen'),\\n\\t\\t\\t\\td.div('units').add(\\n\\t\\t\\t\\t\\td.div('data').add(\\n\\t\\t\\t\\t\\t\\td.p('title').text('Unidades'),\\n\\t\\t\\t\\t\\t\\td.p('quantity').text(this.sumShips())\\n\\t\\t\\t\\t\\t),\\n\\t\\t\\t\\t\\td.div('data').add(\\n\\t\\t\\t\\t\\t\\td.p('title').text('Créditos'),\\n\\t\\t\\t\\t\\t\\td.p(