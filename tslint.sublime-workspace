en; k++) {\\n\\n\\t\\t\\t\\tcostRule[costName][k] = parseFloat(productions[building][costName][k]);\\n\\t\\t\\t}\\n\\t\\t}\\n\\t\\tfinalCosts[building] = costRule;\\n\\t}\\n\\n\\treturn finalCosts;\\n};\\n\\nexports.adjacencys = function(adjacencys) {\\n\\tvar finalAdjacencys = {};\\n\\n\\tfor (var buildingsCombination in adjacencys) {\\n\\t\\tvar finalAdjacencyRules = [];\\n\\n\\t\\tvar adjacencyRules = adjacencys[buildingsCombination];\\n\\t\\tfor (var i = adjacencyRules.length - 1; i >= 0; i--) {\\n\\n\\t\\t\\tvar rule = adjacencyRules[i];\\n\\t\\t\\tvar ruleName = buildingsCombination + '.' + rule.rule;\\n\\t\\t\\tfinalAdjacencyRules.push(modifiers(ruleName, rule));\\n\\t\\t}\\n\\n\\t\\tfinalAdjacencys[buildingsCombination] = finalAdjacencyRules;\\n\\t}\\n\\n\\treturn finalAdjacencys;\\n};\\n\",\"'use strict';\\n\\nvar utils = require('starriser/services/models/decoders/utils.js');\\n\\nexports.decodeSystem = function (data) {\\n\\tvar processed = {};\\n\\n\\tprocessed.id = data.id;\\n\\tprocessed.mame = data.name;\\n\\tprocessed.position = utils.decodePosition(data.position);\\n\\tprocessed.resourceType = data.resourceType;\\n\\tprocessed.stargate = exports.decodeCelestialData(data.stargate.celestialData);\\n\\tprocessed.connections = data.connections; //TODO decode connections\\n\\tprocessed.cluster = data.cluster; //TODO decode cluster cache\\n\\n\\t//processed.position.x += data.cluster.position.x;\\n\\t//processed.position.y += data.cluster.position.y;\\n\\tprocessed.position.x += data.position.x;\\n\\tprocessed.position.y += data.position.y;\\n\\n\\tprocessed.location = {\\n\\t\\tplanetId: null,\\n\\t\\t//clusterId: data.cluster.id,\\n\\t\\tclusterId: data.clusterId,\\n\\t\\tsystemId: data.id\\n\\t};\\n\\n\\tprocessed.planets  = exports.decodePlanets(data.planets);\\n\\n\\treturn processed;\\n};\\n\\nexports.decodePlanets = function (planets) {\\n\\n\\tvar processed = [];\\n\\n\\tif(!planets) return processed;\\n\\n\\tvar len = planets.length;\\n\\tfor (va