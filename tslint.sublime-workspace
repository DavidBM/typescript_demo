ations/ataque.png',\\n\\t\\tfleet: '/assets/img/notifications/ataque.png'\\n\\t},\\n\\tlowCredits: '/assets/img/notifications/ataque.png',\\n\\tplanetObtained: '/assets/img/notifications/ataque.png',\\n\\tshipsBuild: '/assets/img/notifications/ataque.png',\\n\\ttravelFinished: '/assets/img/notifications/ataque.png',\\n\\tfullStorage: '/assets/img/notifications/almacenes_llenos.png',\\n\\tterraformationFailed: '/assets/img/notifications/ataque.png'\\n};\\n\",\"'use strict';\\nvar d = require('dom-wrapper');\\n\\nmodule.exports = exports = GlobalMenuProfile;\\n\\nfunction GlobalMenuProfile (data) {\\n\\n\\tvar obj = {};\\n\\n\\tthis.obj = obj;\\n\\n\\tthis.bus = data.bus;\\n\\n\\tvar dom = d.div('global-menu-profile').add(\\n\\t\\td.div('user').add(\\n\\t\\t\\td.img('image').attr('src', getBigAvatarURL(data.avatar)).save(obj, 'bigAvatar'),\\n\\t\\t\\td.div('data').add(\\n\\t\\t\\t\\td.p('menu-title').text('Usuario'),\\n\\t\\t\\t\\td.p('user-name').text(data.alias ? data.alias : \\\"No name\\\"),\\n\\t\\t\\t\\td.p('change-password').text('Modificar contraseña').save(obj, 'showPasswordChange'),\\n\\t\\t\\t\\td.div('change-password-menu').add(\\n\\t\\t\\t\\t\\td.p('action-title').text('Modificar contraseña'),\\n\\t\\t\\t\\t\\td.input('actual-password').attr('placeholder', 'Contraseña actual').save(obj, 'oldPassword'),\\n\\t\\t\\t\\t\\td.input('new-password').attr('placeholder', 'Nueva contraseña').save(obj, 'fisrtNewPassword'),\\n\\t\\t\\t\\t\\td.input('new-password-repeat').attr('placeholder', 'Repite la nueva contraseña').save(obj, 'secondNewPassword'),\\n\\t\\t\\t\\t\\td.p('cancel').text('Cancelar').save(obj, 'passwordCancel').save(obj, 'cancelPasswordChange'),\\n\\t\\t\\t\\t\\td.p('accept').text('Aceptar').save(obj, 'passwordAccept').event('click', setPassword.bind(this))\\n\\t\\t\\t\\t).save(obj, 'changePasswordZone')\\n\\t\\t\\t),\\n\\t\\t\\td.p('close').text('X').save(obj, 'close')\\n\\t\\t),\\n\\t\\td.div('avatar-selection').add(\\n\\t\\t\\td.p('action-title').te