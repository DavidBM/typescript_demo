es = function (a, b) {\\n\\treturn a.weight - b.weight;\\n};\\n\\nfunction createSearchData (nodes, start) {\\n\\tvar data = createSearchDataNodes(nodes, start);\\n\\taddConnectionDataToSearchNodeData(data);\\n\\n\\tvar startNodeIndex = getIndexByInSearchDataId(data, start.id);\\n\\treturn {data: data, start: data[startNodeIndex]};\\n}\\n\\nfunction addConnectionDataToSearchNodeData (data) {\\n\\tfor (var i = data.length - 1; i >= 0; i--) {\\n\\t\\tvar node = data[i];\\n\\t\\tnode.connections = [];\\n\\n\\t\\tfor (var j = node.node.connections.length - 1; j >= 0; j--) {\\n\\t\\t\\tvar connection = node.node.connections[j];\\n\\t\\t\\tnode.connections.push({\\n\\t\\t\\t\\tindexA: getIndexByInSearchDataId(data, connection.nodeA.id),\\n\\t\\t\\t\\tindexB: getIndexByInSearchDataId(data, connection.nodeB.id),\\n\\t\\t\\t\\tweight: connection.weight,\\n\\t\\t\\t\\tconnection: connection\\n\\t\\t\\t});\\n\\t\\t}\\n\\t}\\n}\\n\\nfunction createSearchDataNodes (nodes, start) {\\n\\tvar data = [];\\n\\tvar index = 0;\\n\\tfor (var i = nodes.length - 1; i >= 0; i--) {\\n\\t\\tvar node = {\\n\\t\\t\\tid: nodes[i].id,\\n\\t\\t\\tweight: -1,\\n\\t\\t\\treferer: null,\\n\\t\\t\\tclosed: false,\\n\\t\\t\\tindex: index,\\n\\t\\t\\tnode: nodes[i]\\n\\t\\t};\\n\\t\\tdata.push(node);\\n\\t\\tindex++;\\n\\t}\\n\\n\\treturn data;\\n}\\n\\nfunction getIndexByInSearchDataId (data, conectedNodeId) {\\n\\tfor (var i = data.length - 1; i >= 0; i--) {\\n\\t\\tif(data[i].node.id === conectedNodeId) return i;\\n\\t}\\n\\treturn false;\\n}\\n\\nfunction closeNode (node) {\\n\\tnode.closed = true;\\n}\\n\\nfunction isOpen (node) {\\n\\treturn (node.closed === false);\\n}\\n\\nfunction isNotExcluded (excluded, node) {\\n\\treturn (excluded.indexOf(node.id) === -1);\\n}\\n\\nfunction getConnectionsQuantity (node) {\\n\\treturn node.node.connections.length;\\n}\\n\\nfunction getNextNode (nodes, actual, index) {\\n\\tif(actual.connections[index].indexA === actual.index)\\n\\t\\treturn nodes[actual.con