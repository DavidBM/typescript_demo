 = function(eventData) {\\n\\tvar event = eventData;\\n\\tif(this.eventFilter !== null) event = this.eventFilter(eventData);\\n\\n\\tfor (var i = event.changedTouches.length - 1; i >= 0; i--) {\\n\\t\\tthis.fingers.removeFinger(event.changedTouches[i], eventData);\\n\\t}\\n};\\n\\nInteraction.prototype.touchmove = function(eventData) {\\n\\tvar event = eventData;\\n\\tif(this.eventFilter !== null) event = this.eventFilter(eventData);\\n\\n\\tfor (var i = event.changedTouches.length - 1; i >= 0; i--) {\\n\\t\\tthis.fingers.moveFinger(event.changedTouches[i], eventData);\\n\\t}\\n};\\n\\nInteraction.prototype.mousewheel = function(event) {\\n\\tevent = wheel.mouseWheelAdaptEvent(event);\\n\\tthis.emit('zoom', event.deltaY, event.pageX, event.pageY);\\n};\\n\\nInteraction.prototype.listenMousewheel = function(element) {\\n\\tvar _this = this;\\n\\twheel.mousewheel(element, function (event) {\\n\\t\\tvar growth = getGrowthFromDeltaY(event.deltaY);\\n\\t\\t_this.emit('zoom', growth, event.pageX, event.pageY);\\n\\t});\\n};\\n\\nInteraction.prototype.setGlobalEvents = function() {\\n\\tvar _this = this;\\n\\tdocument.addEventListener('mousemove', function (event) {\\n\\t\\tmouseMove.call(_this, event);\\n\\t}, false);\\n};\\n\\nfunction saveEventPosition (obj, event) {\\n\\tobj.x = event.pageX;\\n\\tobj.y = event.pageY;\\n}\\n\\nfunction saveDistance (obj, newPosition, lastPosition) {\\n\\tobj.x = newPosition.x - lastPosition.x;\\n\\tobj.y = newPosition.y - lastPosition.y;\\n}\\n\\nfunction getGrowthFromDeltaY (deltaY) {\\n\\tvar growth;\\n\\tif(deltaY > 0){\\n\\t\\tgrowth = Math.pow(1.05, deltaY);\\n\\t}else if(deltaY < 0) {\\n\\t\\tgrowth = Math.pow(0.95, -deltaY);\\n\\t}\\n\\n\\treturn growth;\\n}\\n\\n\\nfunction mouseMove (event, eventData) {\\n\\tif(this.mouseState.down === false) return false;\\n\\tvar state = this.mouseState;\\n\\n\\tsaveEventPosition(state.actualPosition, event);\\n\\tsaveDistance(state.distance, state.actualPosition, state.lastPosition);\\n\\tsa