ation(data.location);\\n\\tprocessed.ships = exports.decodeShips(data.ships);\\n\\tprocessed.owner = utilsDecoders.decodeOwner(data.owner);\\n\\n\\treturn processed;\\n};\\n\\nexports.decodeTravel = function (data) {\\n\\tif(!data) return null;\\n\\tvar processed = {};\\n\\n\\tprocessed.estimatedRemainingTime = data.estimatedRemainingTime;\\n\\tprocessed.route = mapDecoder.decodeRoute(data.route);\\n\\n\\treturn processed;\\n};\\n\\nexports.decodeBehaviour = function (data) {\\n\\tvar processed = {};\\n\\n\\tprocessed.action = data.action;\\n\\tprocessed.state = data.state;\\n\\tif(typeof data.stateRemainingTime !== 'undefined')\\n\\t\\tprocessed.stateRemainingTime = data.stateRemainingTime;\\n\\n\\treturn processed;\\n};\\n\\nexports.decodeShips = function (data) {\\n\\tvar processed = {};\\n\\n\\tfor (var i = SHIPS_TYPES.length - 1; i >= 0; i--) {\\n\\t\\tvar type = SHIPS_TYPES[i];\\n\\t\\tprocessed[type] = 0;\\n\\t\\tif(data && typeof data[type] !== 'undefined')\\n\\t\\t\\tprocessed[type] = data[type];\\n\\t}\\n\\n\\treturn processed;\\n};\\n\\nexports.decodeResources = function (data) {\\n\\tvar processed = {n: 0, a: 0, b: 0, c: 0, d: 0, e: 0, f: 0};\\n\\n\\tif(!data) return processed;\\n\\n\\tfor(var type in processed){\\n\\t\\tprocessed[type] = parseFloat(data[type]);\\n\\t}\\n\\n\\treturn processed;\\n};\\n\\nexports.decodeCreated = function (data) {\\n\\treturn exports.decodeFleet(data);\\n};\\n\\nexports.decodeDestroyed = function (data) {\\n\\tvar processed = {};\\n\\n\\tprocessed.fleetId = data.fleetId;\\n\\tprocessed.reason = data.reason;\\n\\tprocessed.addedTo = data.addedTo;\\n\\n\\treturn processed;\\n};\\n\\nexports.decodePositionUpdated = function (data) {\\n\\tvar processed = {};\\n\\n\\tprocessed.fleetId = data.fleetId;\\n\\tprocessed.location = mapDecoder.decodeLocation(data.location);\\n\\n\\treturn processed;\\n};\\n\\nexports.decodeShipsAdded = function (data) {\\n\\tvar processed = {};\\n\\n\\tprocessed.fleetId = data.fleetId;\\n\\tprocessed.