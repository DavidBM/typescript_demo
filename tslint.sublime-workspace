Data = function(finger) {\\n\\tthis.getEventData();\\n};\\n\\nFingers.prototype.updateFingersQuantity = function() {\\n\\tthis.quantity = this.fingersIds.length;\\n};\\n\\nFingers.prototype.linkFingers = function() {\\n\\tvar keys = this.fingersIds;\\n\\n\\tif(this.quantity === 2)\\n\\t\\tthis.linkFingersInZoom(\\n\\t\\t\\tthis.fingers[keys[0]],\\n\\t\\t\\tthis.fingers[keys[1]]\\n\\t\\t);\\n\\n\\tif(this.quantity > 2)\\n\\t\\tfor (var i = keys.length - 1; i >= 2; i--) {\\n\\t\\t\\tthis.disableFinger(this.fingers[keys[i]]);\\n\\t\\t}\\n};\\n\\nFingers.prototype.checkBrokenLinks = function(finger) {\\n\\tvar linkedFingers = finger.getLinkedFingers();\\n\\n\\tvar isZoom = finger.getType() === 'zoom';\\n\\tvar disabledFingers = this.disabledFingers.length;\\n\\n\\tif(isZoom && disabledFingers > 0){\\n\\t\\tvar disabledFinger = this.disabledFingers.shift();\\n\\t\\tthis.linkFingersInZoom(linkedFingers[0], disabledFinger);\\n\\t\\treturn this.enableFinger(disabledFinger);\\n\\t}\\n\\n\\tif(isZoom && disabledFingers <= 0){\\n\\t\\treturn this.makeFingerDrag(linkedFingers[0]);\\n\\t}\\n\\n\\tfinger.unlinkAll();\\n};\\n\\nFingers.prototype.computeFingerEvent = function(finger, touchEvent) {\\n\\tswitch(finger.getType()) {\\n\\t\\tcase 'drag':\\n\\t\\t\\treturn this.computeDragEvent(finger, touchEvent);\\n\\t\\tcase 'zoom':\\n\\t\\t\\treturn this.computeZoomEvent(finger, touchEvent);\\n\\t}\\n};\\n\\nFingers.prototype.computeDragEvent = function(finger, touchEvent) {\\n\\tthis.emit('drag', finger.getMovementFromEvent(touchEvent), touchEvent);\\n};\\n\\nFingers.prototype.computeZoomEvent = function(finger, touchEvent) {\\n\\tvar secondFinger = finger.getLinkedFingers()[0];\\n\\n\\tvar oldDistance = finger.getDistanceFromFinger(secondFinger);\\n\\tvar center = finger.getCenterWithCopy(secondFinger); //Copy. Make for not autoupdate center when recalculate the new center\\n\\n\\tfinger.update(touchEvent);\\n\\n\\tvar newCenter = finger.getCenterWith(secondFinger);\\n\\tvar newDis