 shipsQuantity = parseInt(this.quantityInputs[type].value);\\n\\tif(isNaN(shipsQuantity) || shipsQuantity < 0) shipsQuantity = 0;\\n\\treturn shipsQuantity;\\n}\\n\",\"'use strict';\\n\\nvar t2t = require('starriser/utils/timeToText.js');\\n\\nfunction ShipInfo (data) {\\n\\tvar d = this.plugins.html;\\n\\n\\tthis.type = data.type;\\n\\tthis.costs = data.costs;\\n\\n\\treturn d.div('shipInfo').addChild([\\n\\t\\td.div('costs').addChild([\\n\\t\\t\\td.p('text').addChild(d.text('Coste unitario')),\\n\\t\\t\\td.p('resource n')\\t.addChild(d.text(this.costs.n)),\\n\\t\\t\\td.p('resource a')\\t.addChild(d.text(this.costs.a)),\\n\\t\\t\\td.p('resource b')\\t.addChild(d.text(this.costs.b)),\\n\\t\\t\\td.p('resource c')\\t.addChild(d.text(this.costs.c)),\\n\\t\\t\\td.p('resource time')\\t.addChild(d.text(t2t(this.costs.time))),\\n\\t\\t\\td.p('resource d')\\t.addChild(d.text(this.costs.d)),\\n\\t\\t\\td.p('resource e')\\t.addChild(d.text(this.costs.e)),\\n\\t\\t\\td.p('resource f')\\t.addChild(d.text(this.costs.f)),\\n\\t\\t\\td.p('resource credits')\\t.addChild(d.text(this.costs.credits + '/h'))\\n\\t\\t])\\n\\t]);\\n}\\n\\nmodule.exports = ShipInfo;\\n\",\"'use strict';\\nvar Chart = require('chart.js');\\n\\nfunction WareHouse (data) {\\n\\tvar d = this.plugins.html;\\n\\n\\tthis.bus = data.bus;\\n\\n\\tthis.tData.resourcesList = {};\\n\\n\\tvar distributionChart = makeDistributionChart.call(this, data.data.resources);\\n\\n\\treturn d.div('warehouse').addChild([\\n\\t\\td.div('distributionChart').addChild([\\n\\t\\t\\tdistributionChart,\\n\\t\\t\\td.div('icons').addChild([\\n\\t\\t\\t\\td.div('resource n'),\\n\\t\\t\\t\\td.div('resource a'),\\n\\t\\t\\t\\td.div('resource b'),\\n\\t\\t\\t\\td.div('resource c'),\\n\\t\\t\\t\\td.div('resource d'),\\n\\t\\t\\t\\td.div('resource e'),\\n\\t\\t\\t\\td.div('resource f')\\n\\t\\t\\t])\\n\\t\\t])\\n\\t]);\\n}\\n\\nmodule.exports = exports = WareHouse;\\n\\nfunction makeDistributionChart (resources) {\\n\\tvar d = this.plugins.htm