Costs = require('starriser/config/balance.json').shipCosts;\\n\\nvar validShipInfoTypes = ['fighter','terraformer','freighter'];\\n\\nfunction Shipyard (data) {\\n\\tvar d = this.plugins.html;\\n\\n\\tthis.bus = data.bus;\\n\\n\\tthis.buttons = {};\\n\\tthis.wastedResources = {};\\n\\tthis.planetId = data.planetId;\\n\\tthis.menus = {};\\n\\n\\tvar buildMenu = TrueDom(shipBuildMenuTemplate, {resources: data.resources, onChange: shipsChanged.bind(this)});\\n\\n\\tvar dom = d.div('shipyard').addChild([\\n\\t\\tmenu.call(this),\\n\\t\\torbitingShips.call(this, data.fleet),\\n\\t\\twastedResources.call(this),\\n\\t\\td.div('container').addChild([\\n\\t\\t\\td.div('build-menu').addChild(buildMenu.execute()).saveIn(this.menus, 'buildMenu'),\\n\\t\\t\\td.div('ship-info').saveIn(this.menus, 'shipInfo')\\n\\t\\t])\\n\\t]);\\n\\n\\tinsertEvents.call(this);\\n\\n\\treturn dom;\\n}\\n\\nmodule.exports = exports = Shipyard;\\n\\nfunction menu () {\\n\\tvar d = this.plugins.html;\\n\\n\\treturn d.div('menu').addChild([\\n\\t\\td.p('item').addChild(d.text('Shipyard')).saveIn(this.buttons, '').setAttr('shipType', 'buildMenu'),\\n\\t\\td.div('spacer'),\\n\\t\\t//d.p('item').addChild(d.text('Destroyer'))   .saveIn(this.buttons, 'destroyer').setAttr('shipType', 'destroyer'),\\n\\t\\td.p('item').addChild(d.text('Fighter'))     .saveIn(this.buttons, 'fighter').setAttr('shipType', 'fighter'),\\n\\t\\t//d.p('item').addChild(d.text('Ranger'))      .saveIn(this.buttons, 'ranger').setAttr('shipType', 'ranger'),\\n\\t\\t//d.p('item').addChild(d.text('Tank'))        .saveIn(this.buttons, 'tank').setAttr('shipType', 'tank'),\\n\\t\\t//d.p('item').addChild(d.text('Mage'))        .saveIn(this.buttons, 'mage').setAttr('shipType', 'mage'),\\n\\t\\t//d.p('item').addChild(d.text('Cruiser'))     .saveIn(this.buttons, 'cruiser').setAttr('shipType', 'cruiser'),\\n\\t\\t//d.p('item').addChild(d.text('Drone'))       .saveIn(this.buttons, 'drone').setAttr('shipType', 'drone'),\\n\\t\\td.p('item').addChild(