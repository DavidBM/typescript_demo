{
  "name": "ships-demo",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./start.js",
    "start:watch": "npm run build && ./node_modules/.bin/tsc-watch --onSuccess \"node ./start.js\"",
    "test": "npm run build:test && node ./test.js",
    "test:watch": "npm run build:test && ./node_modules/.bin/tsc-watch --onSuccess \"node ./test.js\"",
    "build": "./node_modules/.bin/tsc",
    "build:test": "./node_modules/.bin/tsc && cp ./specs/jasmine.json ./dist/specs/jasmine.json",
    "install:clean": "rm -rf node_modules dist"
  },
  "dependencies": {
    "@types/jasmine": "^2.8.3",
    "@types/node": "^8.0.28",
    "reflect-metadata": "^0.1.10",
    "source-map-support": "^0.5.4",
    "tsconfig-paths": "^3.2.0",
    "tspath": "^1.2.10",
    "typescript": "^2.8.1"
  },
  "devDependencies": {
    "tsc-watch": "^1.0.17",
    "jasmine": "^2.8.0"
  }
}
